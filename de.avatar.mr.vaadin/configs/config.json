{
  ":configurator:resource-version": 1,
  
	"VaadinApplication~Vaadin": 
  	{
  	    "vaadin.pwa": "avatar-mr",
  	    "vaadin.application.description": "Avatar Model Repo Application",
		"vaadin.application.name" : "avatar-mr",
		"vaadin.application.context" : "/avatar-mr",
		"vaadin.application.theme": "Material",
		"vaadin.application.frontend": "VITE"
	},
	"EMFLuceneIndex~ePackage": 
  	{
  	    "id": "ePackage",
  	    "directory.type": "MMAP",
		"base.path" : "/tmp/index/ePackage/"
	},
	"ModelDocumentationProvider~docProvider" : {
		"output.root.folder" : "./data/model/documentation/",
		"output.md.folder" : "md",
		"output.md.mermaid.folder" : "md_mermaid",
		"output.md.plantuml.folder" : "md_plantuml",
		"output.html.folder" : "html",
		"output.html.mermaid.folder" : "html_mermaid",
		"output.package.hash.code.folder": "epackage_hash_code"
	},
	"FilePackageLoader~fileLoader": {
		"modelFolder": "./data/model/upload/"
	},
	"GDPRModelEvaluator~multilabelv2": {
		"basePath": "./data/suggester/",
		"modelPath": "./data/suggester/model/",
		"pyScriptBasePath": "./data/suggester/py/multilabel_v2/",
		"outputBasePath": "./data/suggester/out/",
		"modelName": "multilabel_v2"
	},
	"GDPRModelSuggesterRetrainer~multilabelv2": {
		"basePath": "./data/suggester/",
		"pyScriptBasePath": "./data/suggester/py/multilabel_v2/",
		"modelName": "multilabel_v2"
	},
	"GDPRModelEvaluator~spacy": {
		"basePath": "./data/suggester/",
		"modelPath": "./data/suggester/model/",
		"pyScriptBasePath": "./data/suggester/py/spacy/",
		"outputBasePath": "./data/suggester/out/",
		"modelName": "spacy_textcat_ner_negex"
	},
	"GDPREObjectEvaluator~ner_fake_pii_generator": {
		"basePath": "./data/suggester/",
		"modelPath": "./data/suggester/model/",
		"pyScriptBasePath": "./data/suggester/py/ner_fake_pii_generator/",
		"outputBasePath": "./data/suggester/out/",
		"modelName": "ner_fake_pii_generator"
	},
	"GDPRModelSuggesterRetrainer~spacy": {
		"basePath": "./data/suggester/",
		"pyScriptBasePath": "./data/suggester/py/spacy/",
		"modelName": "spacy_textcat_ner_negex"
	},
	"EMFMongoRepositoryConfigurator~avatarmdp": {
		"mongo.instances": "avatarmdp",
		"avatarmdp.baseUris": "mongodb://mongodb",
		"avatarmdp.databases": "avatarmdp",
		"avatarmdp.avatarmdp.repoType": "PROTOTYPE"
	},
	"DBObjectsEvaluator~dbObjEvaluator": {
		"eObjEvaluatorSO.target": "(criterium=GDPR)",
		"repositorySO.target": "(repo_id=avatarmdp.avatarmdp)",
		"evaluation.out.folder": "./data/instance-evaluations/",
		"eClass.uris.to.evaluate": 
			["http://avatar.de/mdp/medical_record_example/1.0.0#//MedicalRecord"
			,"http://avatar.de/mr/person/1.0#//Person"
			]
	}
	
}